                        <label for="">Beneficiary Address</label>
                        <input type="text" name="foreign_address">
                        <label for="">Beneficiary City/State</label>
                        <input type="text" name="region">
                        <label for="">Transaction Description</label>
                        <input type="text" name="foreign_description">
                    </div>
                    <input type="text" value="<%= user.id %>" name="ownby" style=" display:none; ">
                    <div class="btn__wrapper">
                        <button type="submit" class="procceed__btn" id="procceed__btn">Procceed &rarr;</button>
                    </div>
                </form>
            </div>
        </div>
    
        <div class="card__profile user__spec">
            <div class="dashboard__profile">
                <div class="profile__avatar">
                    <img src=<%= user.profile && !user.pending ? user.profile : "/img/default-avatar.png" %> alt="<%= user.username %> avatar" />
                </div>
                <div class="profile__details">
                    <h4><%- first_name %> <%- other_name %></h4>
                    <p>
                        <%= date.toLocaleString() %> | &nbsp;
                        <% if (user.account_status == "on") { %>
                        <p style="color: rgb(11, 189, 11);"> Verified </p>
                            <% } else { %>
                        <p style="color: #ffa500;"> Pending </p>
                            <% } %>
                    </p>
                </div>
                <!-- <hr> -->
                <div class="profile__data">
                    <div class="icon__data">
                        <div class="icon">
                            <img src="/img/layout/user.svg" alt="" srcset="">
                        </div>
                        <span><%= user.first_name + ' ' + user.other_name %> </span>
                    </div>
                    <div class="icon__data">
                        <div class="icon">
    
                            <img src="/img/layout/identification.svg" alt="" srcset="">
                        </div>
                        <span><%= user.user_id %></span>
                    </div>
                    <div class="icon__data">
                        <div class="icon">
                            <img src="/img/layout/email.svg" alt="" srcset="">
                        </div>
                        <span><%= user.email.length > 25 ? user.email.substr(0,25) + "..." : user.email %></span>
                    </div>
                    <div class="icon__data">
                        <div class="icon">
                            <img src="/img/layout/phone-call.svg" alt="" srcset="">
                        </div>
                        <span><%= user.number %></span>
                    </div>
                    <div class="icon__data">
                        <div class="icon">
                            <img src="/img/layout/calendar.svg" alt="" srcset="">
                        </div>
                        <span><%= user.dob.toLocaleString() %></span>
                    </div>
                    <!-- <hr> -->
                    <div class="profile__location">
                        <div class="icon__data">
                            <div class="icon">
                                <img src="/img/layout/placeholder.svg" class="profile__location-img">
                            </div>
                            <span><%= user.address_line1.substr(0,50) %></span>
                        </div>
                        <div class="icon__data">
                            <div class="icon">
                                <img src="/img/icon/globe.png" class="profile__location-img">
                            </div>
                            <span class="user__data"><%= user.state %>, <%= user.country %></span>
                        </div>
                    </div>
                </div>
                <% if (!bank) { %>
                <div class="add__card-home">
                    <a href="/add-card" class="alert-bank">
                        <div class="add__card-img">
                            <img src="/img/layout/add button dark.svg" alt="" srcset="">
                        </div>
                        <p>
                            Add Bank Account
                        </p>
                    </a>
                </div>
                <% }  %>
            </div>
        </div>
    
    </div>

    <div class="transactions_grid">
        <div class="table_container">
            <div class="table_header">
                <h3>Latest transactions</h3>
                <button>Report</button>
            </div>

            <% if(user.available > 0){ %>
                <div id="transaction-table" class="ag-theme-alpine"></div>
             <% } %>
            
        </div>
    </div>
    <!-- table  -->

</div>