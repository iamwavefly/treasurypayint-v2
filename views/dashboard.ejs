    <div class="cards__wrapper">
        <div class="cards">
            <div class="card1">
                <span>Available Balance</span>
                <h2 style="font-size: 1.35rem;"><%= (user.available).toLocaleString('en-US', { style: 'currency', currency: 'USD', }) %></h2>
                <img src="/img/layout/icon-arrow-up.svg" class="icon-arrow-up">
                <p class="arrow-up"><%= Number(user.recent_received).toLocaleString('en-US', { style: 'currency', currency: 'USD', }) %> </p>
                <img src="/img/layout/icon-arrow-down.svg" class="icon-arrow-down">
                <p class="arrow-down">$0.00</p>
            </div>
            <div class="card1">
                <span>Pending Fund</span>
                <h2 style="font-size: 1.35rem;"> <%= (user.pending).toLocaleString('en-US', { style: 'currency', currency: 'USD', }) %></h2>
                <img src="/img/layout/icon-arrow-up.svg" class="icon-arrow-up">
                <p class="arrow-up">$0.00</p>
                <img src="/img/layout/icon-arrow-down.svg" class="icon-arrow-down">
                <p class="arrow-down">$0.00</p>
            </div>
            <div class="card1">
                <span>Reversed Fund</span>
                <h2 style="font-size: 1.35rem;"><%= (user.reversed).toLocaleString('en-US', { style: 'currency', currency: 'USD', }) %></h2>
                <img src="/img/layout/icon-arrow-up.svg" class="icon-arrow-up">
                <p class="arrow-up">$0.00</p>
                <img src="/img/layout/icon-arrow-down.svg" class="icon-arrow-down">
                <p class="arrow-down">$0.00</p>
            </div>
            <div class="card1">
                <span>Tax Fund</span>
                <h2 style="font-size: 1.35rem;"><%= (user.tax).toLocaleString('en-US', { style: 'currency', currency: 'USD', }) %></h2>
                <img src="/img/layout/icon-arrow-up.svg" class="icon-arrow-up">
                <p class="arrow-up">$0.00</p>
                <img src="/img/layout/icon-arrow-down.svg" class="icon-arrow-down">
                <p class="arrow-down">$0.00</p>
            </div>
        </div>
    
