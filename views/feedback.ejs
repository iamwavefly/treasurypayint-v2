                            <div class="card-body">
										
										
										
										
										
										
										
																
					<div id="alertBox">
				<!-- Button trigger modal -->
						<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter" id="popclicker" style="display:none">
						  Launch demo modal
						</button>

						<!-- Modal -->
						<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
						  <div class="modal-dialog modal-dialog-centered" role="document">
							<div class="modal-content">
							  <div class="modal-header">
								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								  <span aria-hidden="true">&times;</span>
								</button>
							  </div>
							  <div class="modal-body">
								<p>Hello, <%- first_name %>, <br>
								Thanks for your message, we will get back to you as soon as possible.</p>
							  </div>
							  <div class="modal-footer">
								<button type="button" class="btn btn-primary" id="btnclicker">Ok</button>
							  </div>
							</div>
						  </div>
						</div>
					</div>
							
										
										
										
										
										
										
										
										
										
										
										
                                <form id="form-msg-service">
											
									<div class="form-group">
                                        <label class="form-label">Account ID<span class="text-danger">*</span></label>
                                        <input type="text" class="form-control" placeholder= <%- account %> readOnly>
                                        <div class="clearfix"></div>
                                    </div>
									<div class="form-group">
                                        <label class="form-label">Fullname</label>
                                        <input type="text" class="form-control" placeholder= "<%- first_name %> <%- other_name %>" readOnly>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="form-group">
                                        <label class="form-label">Email</label>
                                        <input type="email" class="form-control" placeholder="<%- email %>" readOnly>
                                        <div class="clearfix"></div>
                                    </div>
									<div class="form-group">
                                        <label class="form-label">Subject <span class="text-danger">*</span></label>
                                        <textarea class="form-control" placeholder="Please, <%- first_name %>, give full details of your feedback/report here" required></textarea>
                                        <div class="clearfix"></div>
                                    </div>	
									<div class="form-group">
                                        <label class="form-label">Upload file (optional)</label>
                                        <input type="file" class="form-control" value="upload file">
                                        <div class="clearfix"></div>
                                    </div>
									<div class="form-group col-md-4">
										<label class="custom-control custom-checkbox m-0"></span>
											<input type="checkbox" class="custom-control-input" required>
											<span class="custom-control-label">Accept our terms of services?<span class="text-danger"> *</span>
										</label>
									</div>
                                    <button type="Submit" class="btn btn-primary" id="transBtn">Submit</button>
                                </form>
